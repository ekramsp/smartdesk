    def Shader "Transform2d_occlusion" (
        sdrMetadata = {
            string role = "math"
        }
    )
    {
        uniform token info:id = "UsdTransform2d"
        float2 inputs:in.connect = </Material_2/uvReader_st.outputs:result>
